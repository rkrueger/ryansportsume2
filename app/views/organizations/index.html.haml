%h1 Listing organizations
%table
  %tr
    %th Name
    %th Mascot
    %th Location
    %th Level
    %th Sport
    %th
    %th
    %th
  - @organizations.each do |organization|
    %tr
      %td= organization.name
      %td= organization.mascot
      %td= organization.location
      %td= organization.level
      %td= organization.sport
      %td= link_to 'Show', organization
      %td= link_to 'Edit', edit_organization_path(organization)
      %td= link_to 'Destroy', organization, confirm: 'Are you sure?', method: :delete
%br/
= link_to 'New Organization', new_organization_path
